{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "center",
  "title": "Center",
  "description": "Interact with NFTs on any blockchain.",
  "icon": "command-icon.png",
  "author": "enesozturk",
  "access": "public",
  "contributors": [
    "artivilla"
  ],
  "categories": [
    "Media",
    "Productivity",
    "Fun"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "apiKey",
      "type": "password",
      "required": true,
      "title": "API Key",
      "description": "Your Center API key.",
      "placeholder": "See center.app/api/"
    }
  ],
  "commands": [
    {
      "name": "search",
      "title": "Search Collection or Asset",
      "subtitle": "Center NFTs",
      "description": "Search NFT collections or assets with name, description, or address.",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Search query...",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "get-contracts-of-owner",
      "title": "Assets of Owner",
      "subtitle": "Center NFTs",
      "description": "Get NFTs of owner with ENS or contract address.",
      "mode": "view",
      "arguments": [
        {
          "name": "address",
          "placeholder": "ENS or contract address",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "get-asset",
      "title": "Asset Details",
      "subtitle": "Center NFTs",
      "description": "Get NFTs owned by address.",
      "mode": "view",
      "arguments": [
        {
          "name": "address",
          "placeholder": "Contract Address",
          "type": "text",
          "required": true
        },
        {
          "name": "tokenId",
          "placeholder": "Token ID",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "get-collection",
      "title": "Collection Details",
      "subtitle": "Center NFTs",
      "description": "Get NFT collection in the blockchain in milliseconds.",
      "mode": "view",
      "arguments": [
        {
          "name": "address",
          "placeholder": "Contract Address",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.12.5",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^18.8.3",
    "@types/react": "^18.2.61",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "publish:extension": "ray publish"
  }
}
